{
  "hosting": {
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {"source": "/static/**", "headers": [{"key": "Cache-Control", "value": "public,max-age=31536000,immutable"}]}
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "rifa-app",
          "region": "us-central1"
        }
      }
    ]
  }
}

